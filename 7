#include <iostream>
#include <fstream>
void input(int length)
{
	int number_1;
	
	std::ofstream fout("F1.bin", std::ios_base::binary);
	for (int i = 0; i < length;i++)
	{
		std::cin >> number_1;
		fout.write((char*)&number_1, sizeof(int));
	}
	fout.close();
}
int* read(int length) 
{
	int number_2;
	std::ifstream fout("F1.bin", std::ios_base::binary);
	int* mass = new int[length];
	for (int i = 0; i < length; i++)
	{
		fout.read((char*)&number_2, sizeof(int));
		mass[i] = number_2;
	}
	fout.close();
	return mass;
}
void write(int* mass,int length)
{
	bool count = true;
	std::ofstream fout("F2.bin", std::ios_base::binary);

	for (int j = 0; j < length;) 
	{
		for (int i = 0; i < length; i++)
		{
			
			if (count == true)
			{
				if (mass[i] > 0)
				{
					fout.write((char*)&mass[i], sizeof(int));
					count = false;
					
					mass[i] = 0;

					j += 1;
				}
			}
			else
			{
				if (mass[i] < 0)
				{
					fout.write((char*)&mass[i], sizeof(int));
					count = true;
					
					mass[i] = 0;
					j += 1;
				}
			}
		}
	}
	fout.close();

}
void read_F2(int length)
{
	int number;
	std::ifstream fout("F2.bin", std::ios_base::binary);
	for (int i = 0; i < length; i++)
	{
		fout.read((char*)&number, sizeof(int));
		std::cout << number << " ";
	}
	fout.close();

}

int main()
{
	int length;
	std::cin >> length;
	input(length);
	write(read(length),length);
	read_F2(length);
	
}
